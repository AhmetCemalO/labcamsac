**.egg-info/

# Node artifact files
node_modules/
dist/

# vscode IDE
.vscode/

# Compiled Java class files
*.class

# Compiled Python bytecode
*.py[cod]

# Log files
*.log

# Package files
*.jar

# Maven
target/
dist/

# JetBrains IDE
.idea/

# Unit test reports
TEST*.xml

# Generated by MacOS
.DS_Store

# Generated by Windows
Thumbs.db

# Applications
*.app
*.exe
*.war

# Large media files
*.mp4
*.tiff
*.avi
*.flv
*.mov
*.wmv

# PyInstaller build artifacts
build_neucams/build/
build_neucams/dist/
build_neucams/payload.zip

# Large DLLs and binaries (unless you need them for the build)
build_neucams/_internal/
#build_neucams/vmbpy/
#build_neucams/gentl/

# Installer output
*.exe

# Python cache
__pycache__/
*.pyc

# Jupyter notebook checkpoints
.ipynb_checkpoints/

